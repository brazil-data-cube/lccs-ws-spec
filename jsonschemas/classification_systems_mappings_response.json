{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "localhost/lccs_ws/classification_systems_mappings_response.json",
  "type": "object",
  "title": "LCCS-WS - Classification Systems Mappings operation",
  "description": "Retrieves list of offered classification systems mappings",
  "required": [
    "mappings"
  ],
  "mappings": {
      "$id": "#/properties/mappings",
      "type": "array",
      "title": "Classification Systems Mappings",
      "description": "Defines classification systems classes mappings from server",
      "default": false,
      "items": {
        "$id": "#/properties/mappings/items",
        "type": "string",
        "title": "Classification Systems Mapping Items",
        "description": "List of Classification Systems Classes Mappings",
        "required": [
          "source_class_id",
          "source_class_name",
          "target_class_id",
          "target_class_name",
          "description",
          "degree_of_similarity"
        ],
        "properties":{
          "source_class_id":{
            "$id": "#/properties/mappings/items/properties/source_class_id",
            "type": "number",
            "title": "Source Class Identificator",
            "default": "",
            "examples": [
              2
            ]
          },
          "source_class_name":{
            "$id": "#/properties/mappings/items/properties/source_class_name",
            "type": "string",
            "title": "Source Class Name",
            "default": "",
            "examples": [
              "Floresta Prim√°ria"
            ]

          },
          "target_class_id":{
            "$id": "#/properties/mappings/items/properties/target_class_id",
            "type": "number",
            "title": "Target Class Identificator",
            "default": "",
            "examples": [
              3
            ]
          },
          "target_class_name":{
            "$id": "#/properties/mappings/items/properties/target_class_name",
            "type": "string",
            "title": "Target Class Name",
            "default": "",
            "examples": [
              "Floresta"
            ]
          },
          "description":{
            "$id": "#/properties/mappings/items/properties/description",
            "type": "string",
            "title": "Description of Classification Systems Classes Mapping",
            "default": ""
          },
          "degree_of_similarity":{
            "$id": "#/properties/mappings/items/properties/degree_of_similarity",
            "type": "number",
            "title": "Degree of similarity of Classification Systems Classes Mapping",
            "default": "",
            "examples": [
              0.2
            ]
          }

        }
      }
    }
}